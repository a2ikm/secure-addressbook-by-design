<div id="<%= dom_id contact %>">
  <p>
    <strong>First name:</strong>
    <%= contact.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= contact.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>Zipcode:</strong>
    <%= contact.zipcode %>
  </p>

  <p>
    <strong>State:</strong>
    <%= contact.state %>
  </p>

  <p>
    <strong>City:</strong>
    <%= contact.city %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= contact.street %>
  </p>

  <p>
    <strong>Building:</strong>
    <%= contact.building %>
  </p>

</div>
